{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "env USE_SYSTEM_FPM=true electron-builder --linux ",
    "build:delete": "rimraf ./dist && env USE_SYSTEM_FPM=true electron-builder --linux ",
    "build:usebuildconfigjs": "rimraf ./dist && env USE_SYSTEM_FPM=true electron-builder --linux --config ./.build/build.config.js"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "21.4.4",
    "electron-builder": "^24.13.3"
  }
}
